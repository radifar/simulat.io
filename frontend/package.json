    "url": "http://radifar-dsl.id"
  },
  "private": true,
  "scripts": {
    "serve": "npm i && vue-cli-service serve",
    "build": "vue-cli-service build",
    "e2e": "node test/e2e/runner.js",
    "lint": "vue-cli-service lint",
    "dev": "npm i && vue-cli-service serve",
    "lint-fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.1",
    "chart.js": "^2.7.3",
    "core-js": "^2.6.5",
    "file-saver": "^2.0.2",
    "register-service-worker": "^1.5.2",
    "rete": "^1.4.2",
    "rete-area-plugin": "^0.2.1",
    "rete-auto-arrange-plugin": "^0.4.0",
    "rete-code-plugin": "^0.2.0",
    "rete-comment-plugin": "^0.6.1",
    "rete-connection-mastery-plugin": "^0.2.3",
    "rete-connection-path-plugin": "^0.3.1",
    "rete-connection-plugin": "^0.9.0",
    "rete-connection-reroute-plugin": "^0.4.0",
    "rete-context-menu-plugin": "^0.5.2",
    "rete-dock-plugin": "^0.2.1",
    "rete-history-plugin": "^0.2.1",
    "rete-minimap-plugin": "^0.3.1",
    "rete-module-plugin": "^0.4.0",
    "rete-profiler-plugin": "^0.2.0",
    "rete-task-plugin": "^0.2.0",
    "rete-vue-render-plugin": "^0.5.0",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.2",
    "vue-chartjs": "^3.4.0",
    "vue-i18n": "^8.8.1",
    "vue-router": "^3.1.3",
    "vue-router-prefetch": "^1.6.0",
    "vue2-transitions": "^0.3.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "graphql-tag": "^2.9.2",
    "vue-cli-plugin-apollo": "^0.16.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-template-compiler": "^2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ]
  }
}
